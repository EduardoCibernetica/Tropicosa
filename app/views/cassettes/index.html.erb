<p id="notice"><%= notice %></p>

<h1>Cassettes Tropicosa</h1>

<table>
  <thead>
    <tr>
      <th>Titulo</th>
      <th>Autor</th>
      <th>Ano</th>
      <th>Disquetera</th>
      <th>Precio</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cassettes.each do |cassette| %>
      <tr>
        <td><%= cassette.Titulo %></td>
        <td><%= cassette.Autor %></td>
        <td><%= cassette.Ano %></td>
        <td><%= cassette.Disquetera %></td>
        <td><%= cassette.Precio %></td>
        <td><%= link_to 'Mostrar', cassette %></td>
        <td><%= link_to 'Editar', edit_cassette_path(cassette) %></td>
        <td><%= link_to 'Eliminar', cassette, method: :delete, data: { confirm: 'Estas Seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Agregar un nuevo cassette', new_cassette_path %>
